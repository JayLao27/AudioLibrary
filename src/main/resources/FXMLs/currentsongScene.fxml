         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="artistName" alignment="CENTER" layoutX="85.0" layoutY="39.0" prefHeight="17.0" prefWidth="290.0" text="Artist Name" />
      <Slider fx:id="playbackSlider" disable="true" layoutX="85.0" layoutY="59.0" prefHeight="14.0" prefWidth="290.0" />
   </children>
</AnchorPane>
